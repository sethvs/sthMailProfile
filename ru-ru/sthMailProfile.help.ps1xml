            </maml:para>
        </maml:description>
        <command:syntax>
            <command:syntaxItem>
                <maml:name>Remove-sthMailProfile</maml:name>
                <command:parameter required="true" variableLength="true" globbing="true" pipelineInput="False" position="0" aliases="none">
                    <maml:name>ProfileName</maml:name>
                    <maml:Description>
